<script>
$(document).ready(function(){
	
	$("#mainContainer").on("click", "#rightArrow", function(){
	
		$("#nextToGoArea").animate({scrollLeft:875},800);
	
	});

$("#mainContainer").on("click", "#leftArrow", function(){

		$("#nextToGoArea").animate({scrollLeft:0},800);

	});

	setInterval(function() {

		$.get( "/includes/nextToGo.php").done(function( data ) {

			$("#nextToGo").replaceWith(data);

	});

	}, 60 * 1000);

	$("#searchResults").hide(); 

	var delay = (function(){
	  var timer = 0;
	  return function(callback, ms){
	    clearTimeout (timer);
	    timer = setTimeout(callback, ms);
	  };
	})();

	var minLength = 2;
	$("#greyhoundSearchInput").keyup(function() {
	delay(function(){ 
		$("#greyhoundSearchResults").width($(".searchBox").width()-18);
		$("#greyhoundSearchResults").empty();
   		$("#greyhoundSearchResults").show();
   		$("#greyhoundSearchResults").text("Searching.....");
   		$("#greyhoundSearchResults").append("<div class=\"loader\"></div>");
		var keyword = $("#greyhoundSearchInput").val();
		if (/[^a-zA-Z.'\s]/.test(keyword) == true) {
			$("#greyhoundSearchResults").text("Invalid characters, try again.");
		}
		else {
			keyword = keyword.toLowerCase();
            var convertedKeyword = keyword;
			convertedKeyword = convertedKeyword.replace("", "'");
			if (keyword.length >= minLength) {
				$.get( "/includes/apiBridgeSearch.php", { searchName: convertedKeyword } )
				  .done(function( data ) {
				    var searchResults = jQuery.parseJSON(data);
				    $("#greyhoundSearchResults").empty();
				    $(searchResults).each(function(key, value) {
				    	keyword = decodeURIComponent(convertedKeyword);
				    	if (value['name'].indexOf(keyword) !== -1) {
				    		var convertedName = value['name'].replace(/\s/g, "-");
				    		convertedName = convertedName.replace("_", "-");
				    		var ucName = value['name'].replace("_", " ");
				    		var whelpDate = value['whelped'];
				    		if (value['active'] == "yes") {
				    			$("#greyhoundSearchResults").append("<div class=\"searchResultName\"><a href=\"/greyhounds/"+convertedName+"\">"+ucName+"</a><div style=\"float: right;\">"+whelpDate+"</div></div>");
				    		}
				    		else {
				    			$("#greyhoundSearchResults").append("<div class=\"searchResultName inactive\"><a href=\"/greyhounds/"+convertedName+"\">"+ucName+"</a><div style=\"float: right;\">"+whelpDate+"</div></div>");
				    		}
				    		
				    	}
				    
			    	})
			    if (keyword.length >= 5) {
			    	$("#greyhoundSearchResults").append("<div class=\"fullResultsLink\"><a href=\"/search/?searchName="+convertedKeyword+"&type=greyhounds\">Full search results</a></div>");	
			    }
			  });
			}
		}
	}, 1000 );
	});

	var trainerMinLength = 3;
	$("#trainerSearchInput").keyup(function() {
	delay(function(){ 
		$("#trainerSearchResults").width($(".searchBox").width()-18);
		$("#trainerSearchResults").empty();
   		$("#trainerSearchResults").show();
   		$("#trainerSearchResults").text("Searching.....");
   		$("#trainerSearchResults").append("<div class=\"loader\"></div>");
		var keyword = $("#trainerSearchInput").val();
		if (/[^a-zA-Z'\s]/.test(keyword) == true) {
			$("#trainerSearchResults").text("Invalid characters, try again.");
		}
		else {
			keyword = keyword.toLowerCase();
            var convertedKeyword = keyword;
			if (keyword.length >= minLength) {
				$.get( "/includes/apiBridgeTrainerSearch.php", { searchName: keyword } )
				  .done(function( data ) {
				    console.log(data);
				    var searchResults = jQuery.parseJSON(data);
				    $("#trainerSearchResults").empty();
				    $(searchResults).each(function(key, value) {
			    		var convertedName = value['name'].replace(/\s/g, "-");
			    		var suburb = value['suburb'];
			    		var ucName = value['name'].replace("_", " ");
	    				$("#trainerSearchResults").append("<div class=\"searchResultName\"><a href=\"/trainers/"+convertedName+"\">"+ucName+"</a><div style=\"float: right;\">"+suburb+"</div></div>");
			    	
			    	})
			    if (keyword.length >= 5) {
			    	$("#trainerSearchResults").append("<div class=\"fullResultsLink\"><a href=\"/search/?searchName="+convertedKeyword+"&type=trainers\">Full search results</a></div>");
			    }
			  });
			}
		}
	}, 1000 );
	});
	
	$("body").not(".searchResults").click(function() {
		$(".searchResults").hide();
	});
	$("#menuFieldsForm").addClass("active");

});
</script>

</body>
</html>